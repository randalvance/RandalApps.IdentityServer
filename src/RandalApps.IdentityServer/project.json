{
  "dependencies": {
    "IdentityServer4": "1.0.0-rc2",
    "IdentityServer4.EntityFramework": "1.0.0-rc2",
    "IdentityServer4.AspNetIdentity": "1.0.0-rc2",
    "Microsoft.NETCore.App": {
      "version": "1.0.1",
      "type": "platform"
    },
    "Microsoft.AspNetCore.Diagnostics": "1.0.0",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.0",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.1",
    "Microsoft.Extensions.Logging.Console": "1.0.0",
    "Microsoft.Extensions.Configuration.FileExtensions": "1.1.0-alpha1-22284",
    "Microsoft.Extensions.Configuration.Json": "1.1.0-alpha1-22284",
    "Microsoft.Extensions.Configuration.UserSecrets": "1.1.0-alpha1-22284",
    "Microsoft.EntityFrameworkCore": "1.1.0-alpha1-22284",
    "Microsoft.AspNetCore.Identity.EntityFrameworkCore": "1.1.0-alpha1-22284",
    "Microsoft.EntityFrameworkCore.SqlServer": "1.1.0-alpha1-22284",
    "Microsoft.AspNetCore.Mvc": "1.1.0-alpha1-22284",
    "Microsoft.Extensions.Logging.Debug": "1.1.0-alpha1-22284",
    "Microsoft.AspNetCore.Diagnostics.EntityFrameworkCore": "1.1.0-alpha1-22284",
    "Microsoft.AspNetCore.StaticFiles": "1.1.0-alpha1-22284"
  },

  "tools": {
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": "1.0.0-preview2-final"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dotnet5.6",
        "portable-net45+win8"
      ]
    }
  },

  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true
  },

  "runtimeOptions": {
    "configProperties": {
      "System.GC.Server": true
    }
  },

  "publishOptions": {
    "include": [
      "wwwroot",
      "Views",
      "appsettings.json",
      "idsvr3test.pfx",
      "web.config"
    ]
  },

  "scripts": {
    "postpublish": [ "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%" ]
  }
}
